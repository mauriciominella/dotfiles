// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": ", b",
        "command": "workbench.action.maximizeEditor"
    },
    {
        "key": ", e",
        "command": "workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.view.explorer"
    },
    {
        "key": "j",
        "command": "list.focusDown",
        "when": "listFocus"
    },
    {
        "key": "down",
        "command": "-list.focusDown",
        "when": "listFocus"
    },
    {
        "key": "k",
        "command": "list.focusUp",
        "when": "listFocus"
    },
    {
        "key": "up",
        "command": "-list.focusUp",
        "when": "listFocus"
    },
    {
        "key": "l",
        "command": "list.expand",
        "when": "listFocus"
    },
    {
        "key": "right",
        "command": "-list.expand",
        "when": "listFocus"
    },
    {
        "key": "h",
        "command": "list.collapse",
        "when": "listFocus"
    },
    {
        "key": "left",
        "command": "-list.collapse",
        "when": "listFocus"
    },
    {
        "key": "m m",
        "command": "renameFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "f2",
        "command": "-renameFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "m d",
        "command": "deleteFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "shift+delete",
        "command": "-deleteFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "m a",
        "command": "explorer.newFile"
    },
    {
        "key": ", f",
        "command": "workbench.files.action.focusFilesExplorer"
    },
    {
        "key": "m d",
        "command": "explorer.newFolder"
    }
]